var firstPerson = prompt("1st person; enter a number between 1-6");
if ((firstPerson < 1) || (firstPerson >6) || (isNaN(firstPerson))) {
  window.alert("you cannot follow directions!!!");
}
else {
var secondPerson = prompt("2nd person; enter a number between 1-6");
if (firstPerson == secondPerson) {
  window.alert("you win!!!");
}
else {
  window.alert("player one answer was " + firstPerson);
  window.alert("you chose " + secondPerson)
  }
}
